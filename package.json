    "type": "git",
    "url": "https://github.com/sambacha/progress-bar-4-axios.git"
  },
  "license": "MIT",
  "keywords": [
    "axios",
    "http",
    "progress",
    "bar",
    "loading",
    "react",
    "reactjs",
    "vue",
    "vuejs",
    "web"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "css-loader": "^0.28.7",
    "jsdom": "^16.5.1",
    "mini-css-extract-plugin": "^0.9.0",
    "prettier": "^2.2.1",
    "tape": "^5.2.2",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "peerDependencies": {
    "axios": "^0.21.1"
  },
  "dependencies": {
    "ajv": "^6.12.6",
