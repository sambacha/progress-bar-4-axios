{
  "name": "axios-progress-bar",
  "version": "1.3.0",
  "description": "Slim progress bar (NProgress) for Web applications that use Axios library for HTTP requests.",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx webpack --progress",
    "test:browser": "npx serve -s -o -p 5000",
    "module:sync": "cp node_modules/nprogress/css/nprogress.css $PWD/src"
  },
